            <output expression="&quot;How many quarters?&quot;" newline="True"/>
            <declare name="amountQuarter" type="Integer" array="False" size=""/>
            <input variable="amountQuarter"/>
            <output expression="&quot;Total amount: $&quot; &amp; ((penny * amountPenny)+ (nickel * amountNickel) + (dime * amountDime) + (quarter * amountQuarter)) / 100" newline="True"/>
