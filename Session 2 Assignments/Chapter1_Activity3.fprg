    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="penny" type="Integer" array="False" size=""/>
            <declare name="nickel" type="Integer" array="False" size=""/>
            <declare name="dime" type="Integer" array="False" size=""/>
            <declare name="quarter" type="Integer" array="False" size=""/>
            <assign variable="penny" expression="1"/>
            <assign variable="nickel" expression="5"/>
            <assign variable="dime" expression="10"/>
            <assign variable="quarter" expression="25"/>
            <output expression="&quot;How many pennies?&quot;" newline="True"/>
            <declare name="amountPenny" type="Integer" array="False" size=""/>
